#include<stdio.h>

int main()
{
	int x = 0;
	int m = 0; 
	int n = 0;
	int i = 0; 
	int j = 0;

	for (i = 2; i < 84; i++)
	{
		if (168 % i == 0)
		{
			j = 168 / i;
			if (i > j)
			{
            m = (i+j) / 2;
			n = (i-j) / 2;
			x = n * n - 100;
            printf("x=%d\n", x);
			}
			
			
		}

	}






	return 0;
}
